#import "KPMessageRowData.h"

@implementation KPMessageRowData

+ (instancetype)initWithTitle:(NSString *)title
{
    KPMessageRowData *rowData = [[KPMessageRowData alloc] init];
    rowData.titleText = title;
    rowData.imgStr = title;
    return rowData;
}


@end
