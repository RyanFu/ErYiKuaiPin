#import "KPMiddleLineLabel.h"

@implementation KPMiddleLineLabel

- (void)drawRect:(CGRect)rect {
    [super drawRect:rect];
    
    UIBezierPath *linePath = [UIBezierPath bezierPathWithRect:CGRectMake(0, self.bounds.size.height * 0.5 - 0.5, self.bounds.size.width, 1)];
    [self.textColor set];
    [linePath fill];
    
}

@end
